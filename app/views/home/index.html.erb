<h1>CALENDAR</h1>

<%= month_calendar events: @meetings do |date, meetings| %>
  <%= date %>

  <% meetings.each do |meeting| %>
    <div>
      <%= link_to meeting.name, meeting %>
      <%= meeting.start_time %>  
    </div>
  <% end %>
<% end %>

<% if user_signed_in? %>
  <%= link_to "Set Sparring Time", meetings_path %>
<% end %>

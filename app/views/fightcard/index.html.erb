<h1>FIGHT CARD</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Image</th>
      <th scope="col">Name</th>
      <th scope="col">Experience</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr scope="row">
        <td><%= image_tag user.image.url(:thumb) %></td>
        <td><%= user.username %></td>
        <td><%= user.experience %></td>
      </tr>
    <% end %>
  </tbody>
</table>


